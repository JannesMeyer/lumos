#!/usr/bin/env node --harmony
'use strict';

/*
 * Installation:
 * LUMOSPATH=~/lumos
 * ln -s $LUMOSPATH/bin/diary /usr/local/bin
 */

const traceur = require('traceur');
traceur.require.makeDefault(function(filename) {
	return filename.startsWith(__dirname + '/../dist');
});
const open = require('../dist/open');

let days = process.argv.slice(2);
open.openDiary(days);