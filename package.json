    "express": "4.x",
    "express-helpers": "1.x",
    "highlight.js": "8.x",
    "marked": "0.x",
    "minimist": "0.x",
    "morgan": "1.x",
    "react": "^0.10.0",
    "static-favicon": "1.x",
    "stylus": "0.x",
    "traceur": "0.x"
  },
  "devDependencies": {
    "browserify": "^4.1.8",
    "chokidar": "^0.8.2",
    "commander": "2.x",
    "connect-livereload": "0.x",
    "gulp": "3.x",
