{
  "name": "lumos",
  "version": "1.0.0",
  "author": "Jannes Meyer <jannes.meyer+npm@gmail.com>",
  "engine": "node 0.11.13",
  "private": true,
  "bin": "./bin/lumos",
  "scripts": {
    "start": "./bin/lumos serve",
    "prepublish": "gulp build"
  },
  "config": {
    "defaultPort": 9000,
    "mdSuffix": ".md",
    "indexFile": "index.md",
    "editURLProtocol": "lumos-connect://",
    "editor": "subl",
    "editorArgs": [
      "--project",
      "/Users/jannes/Dropbox/Notes/notes.sublime-project"
    ],
    "diaryBaseDir": "/Users/jannes/Dropbox/Notes/Tagebuch"
  },
  "dependencies": {
    "body-parser": "1.x",
    "cookie-parser": "1.x",
    "ejs": "0.x",
    "ejs-locals": "1.x",
    "express": "4.x",
    "highlight.js": "8.x",
    "marked": "0.x",
    "minimist": "0.x",
    "morgan": "1.x",
    "traceur": "0.0.43"
  },
  "devDependencies": {
    "es6-shim": "0.x",
    "gulp": "3.x",
    "gulp-autoprefixer": "0.x",
    "gulp-changed": "0.x",
    "gulp-livereload": "1.x",
    "gulp-minify-css": "0.x",
    "gulp-notify": "1.x",
    "gulp-plumber": "0.x",
    "gulp-rename": "1.x",
    "gulp-stylus": "1.x",
    "gulp-traceur": "0.x",
    "gulp-util": "2.x",
    "jsx-loader": "0.10.2",
    "node-notifier": "^3.0.3",
    "vinyl-source-stream": "0.x",
    "webpack": "1.x"
  },
  "os": [
    "darwin"
  ],
  "preferGlobal": true
}
